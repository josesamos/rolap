citHeader("To cite rolap in publications use:")

bibentry(entry = "Manual",
  title        = "{rolap}: Obtaining ROLAP Star Databases from Flat Tables",
  author       = c(as.person("Jose Samos"), as.person("Maria Bermudez-Edo"), as.person("David Sides")),
  year         = "2023",
  note         = "R package version 1.0.0",
  url          = "https://github.com/josesamos/rolap",

  textVersion  =
  paste("Jose Samos, Maria Bermudez-Edo, David Sides (2023).",
        "rolap: Obtaining ROLAP Star Databases from Flat Tables.",
        "https://github.com/josesamos/rolap.")
)
